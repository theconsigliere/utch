            }
          },
          "97466df3-5176-4610-8f70-d982f2211bb3": {
            "type": "testimonial-slide",
            "settings": {
              "background_color": "#e6e7c6",
              "review_stars": 5,
              "desc": "Wonderful shop, so happy to have it practically on my doorstep. Step into a magical jungle, and immerse yourself in the beauty! The staff are so helpful and really know their stuff. I could spend hours in here, it's so tranquil and interesting. Worth a visit",
              "name": "Tabitha Scott"
            }
          },
          "36281280-450e-4fba-b556-de47cafb1f78": {
